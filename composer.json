{
  "name": "vianetz/asyncqueue-magento1",
  "description": "The vianetz AsyncQueue Magento 1 Extension.",
  "type": "magento-module",
  "keywords": [ "vianetz", "magento", "openmage" ],
  "homepage": "https://www.vianetz.com/",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Christoph Massmann",
      "email": "cm@vianetz.com",
      "homepage": "https://www.vianetz.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@vianetz.com",
    "faq": "https://www.vianetz.com/en/magento-faq"
  },
  "require": {
    "php": ">=5.6|^7|^8"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "magento/marketplace-eqp": ">=4.0"
  },
  "suggest": {
    "vianetz/automaticinvoice-magento1": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "extra": {
    "map": [
      [ "src/app/code/community/Vianetz/*", "app/code/community/Vianetz/" ],
      [ "src/app/etc/modules/*", "app/etc/modules/" ]
    ]
  }
}
